{
	"name": "@wvhulle/object",
	"version": "0.0.2",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"repository": "https://github.com/wvhulle/object",
	"keywords": [
		"Node",
		"library",
		"TypeScript"
	],
	"scripts": {
		"build": "tsc && tsc-alias",
		"biome": "biome format --write ./lib" ,
		"coverage": "vitest run --coverage --passWithNoTests",
		"prepublishOnly": "npm run biome && npm run build && publint",
		"test": "vitest --passWithNoTests run"
	},
	"author": "Willem Vanhulle <willemvanhulle@gmail.com>",
	"description": "A library that deals with JavaScript objects in TypeScript",
	"license": "GPL-3.0-only",
	"files": [
		"dist",
		"lib"
	],
	"type": "module",
	"dependencies": {
		"decimal.js": "10.4.3",
		"lodash-es": "4.17.21",
		"tsc-alias": "1.8.10",
		"typescript": "5.5.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.2",
		"@types/lodash-es": "4.17.12",
		"@types/node": "^20.14.6",
		"@wvhulle/tsconfig": "0.0.1",
		"fast-check": "3.19.0",
		"publint": "0.2.8",
		"vitest": "1.6.0"
	}
}
