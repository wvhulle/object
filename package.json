{
	"name": "@wvhulle/object",
	"version": "0.0.1",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"repository": "https://github.com/wvhulle/object",
	"keywords": [
		"Node",
		"library",
		"TypeScript"
	],
	"scripts": {
		"build": "tsc && tsc-alias",
		"coverage": "vitest run --coverage --passWithNoTests",
		"eslint": "eslint -f visualstudio --cache --ext .ts,.js  --ignore-path .gitignore  .",
		"eslint:fix": "eslint -f visualstudio --cache --ext .ts,.js  --ignore-path .gitignore  --fix --fix-type directive,problem,suggestion,layout  .",
		"format": "npm run prettier; npm run eslint:fix",
		"lint": "npm run eslint",
		"prepublishOnly": "npm run format && npm run lint && npm run build && publint",
		"prettier": "prettier --config .prettierrc --cache --cache-location=.prettiercache --ignore-path .gitignore --write  .  ",
		"test": "vitest --passWithNoTests run"
	},
	"author": "Willem Vanhulle <willemvanhulle@gmail.com>",
	"description": "A library that deals with JavaScript objects in TypeScript",
	"license": "GPL-3.0-only",
	"files": [
		"dist",
		"lib"
	],
	"type": "module",
	"dependencies": {
		"decimal.js": "10.4.3",
		"lodash-es": "4.17.21",
		"tsc-alias": "1.8.10",
		"typescript": "5.5.2"
	},
	"devDependencies": {
		"@types/lodash-es": "4.17.12",
		"@types/node": "^20.14.6",
		"@wvhulle/eslint-config-typescript": "0.0.3",
		"@wvhulle/prettier-config-node": "0.0.1",
		"@wvhulle/tsconfig": "0.0.1",
		"eslint": "9.5.0",
		"fast-check": "3.19.0",
		"publint": "0.2.8",
		"vitest": "1.6.0"
	}
}
